    <PageContent>
        <FixedPage xmlns="http://schemas.microsoft.com/xps/2005/06" Width="1056"
Height="816" xml:lang="pl-PL">
            <Canvas>
                <Canvas.RenderTransform>
                    <MatrixTransform Matrix="1.333333333,0,0,1.333333333,0,0"/>
                </Canvas.RenderTransform>
                <Glyphs Name="a0" BidiLevel="0" Fill="#FF000000"
  
  FontRenderingEmSize="11.04" StyleSimulations="None" OriginX="756.7"
  OriginY="546.82" UnicodeString=" " Indices="">
                </Glyphs>
                <Path Data="M 36,36 L 756.65,36 L 756.65,546.74 L 36,546.74 Z"
  Clip="M -0.000000319,36 L 792,36 L 792,558.72 L -0.000000319,558.72 Z ">
                    <Path.Fill>
                        <ImageBrush ImageSource="DeliveryNoteSketch.jpg"
    Viewbox="0,0,1085,769" TileMode="None" ViewboxUnits="Absolute"
    ViewportUnits="Absolute" Viewport="0,0,1,1">
                            <ImageBrush.Transform>
                                <MatrixTransform Matrix="720.65,0,0,510.74,36,36"/>
                            </ImageBrush.Transform>
                        </ImageBrush>
                    </Path.Fill>
                </Path>
                <Path Data="M 66.75,144 L 341.25,144 L 341.25,202.5 L 66.75,202.5 Z ">
                    <Path.Fill>
                        <SolidColorBrush Color="#FFFFFFFF"/>
                    </Path.Fill>
                </Path>
                <Path Data="M 66.75,144 L 341.25,144 L 341.25,202.5 L 66.75,202.5 Z "
  Stroke="#FF000000" StrokeThickness="0.75" StrokeMiterLimit="8">
                </Path>

                <Canvas
  Clip="M 67.08,147.96 L 340.8,147.96 L 340.8,198.48 L 67.08,198.48 L 67.08,147.96 Z ">
                    <Glyphs Name="a1" BidiLevel="0" Fill="#FF000000"

   FontRenderingEmSize="11.04" StyleSimulations="None" OriginX="74.28"
   OriginY="158.54" UnicodeString="tttttttt"
   Indices=",33.696;,33.696;,33.696;,33.696;,32.609;,33.696;,33.696;">
                    </Glyphs>
                </Canvas>
                <Canvas
  Clip="M 67.08,147.96 L 340.8,147.96 L 340.8,198.48 L 67.08,198.48 L 67.08,147.96 Z ">
                    <Glyphs Name="a2" BidiLevel="0" Fill="#FF000000"

   FontRenderingEmSize="11.04" StyleSimulations="None" OriginX="103.82"
   OriginY="158.54" UnicodeString=" " Indices="">
                    </Glyphs>
                </Canvas>
            </Canvas>
        </FixedPage>
    </PageContent>

