                <StackPanel Margin="1.1cm,0.35cm,0,0" Orientation="Horizontal">
                    <StackPanel Orientation="Vertical">

                        <Border Style="{StaticResource notCalledBorder}">
                            <TextBlock Width="7.65cm" Height="0.90cm" Text="q_Client_q"/>
                        </Border>
                        <Border Style="{StaticResource notCalledBorder}">
                            <TextBlock Margin="0,0.4cm,0,0" Width="7.65cm" Height="0.90cm" Text="q_Building_q"/>
